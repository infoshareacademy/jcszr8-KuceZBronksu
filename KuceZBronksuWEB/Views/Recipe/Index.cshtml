@model IEnumerable<RecipeViewModel>

@{
    ViewData["Title"] = "Search";
}

<h1>Search</h1>
<div>
    @foreach (var comment in Model)
    {
        @Html.Partial("~/Views/Shared/_SearchPanel.cshtml",comment.SearchViewModel)
        break;
    }
</div>

<div>
    <partial name="~/Views/Shared/_SearchResult.cshtml" />
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
