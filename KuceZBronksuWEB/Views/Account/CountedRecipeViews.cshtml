@model IEnumerable<KuceZBronksuBLL.Models.CountedRecipeViewsModel>


@{
    ViewData["Title"] = "CountedRecipeViews";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="table">
    <thead>
        <tr>
            <th>
                Nazwa przepisu
            </th>
            <th>
                Ilość kliknięć
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.LabelOfRecipe)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Count)
                </td>
            </tr>
        }
    </tbody>
</table>
